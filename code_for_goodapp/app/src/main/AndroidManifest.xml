        android:icon="@drawable/isapisap"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".farmer_login" />
        <activity android:name=".center_head_login" />
        <activity android:name=".farmer_select" />
        <activity android:name=".showdetails" />
        <activity android:name=".time_picker" />
        <activity android:name=".ConfirmationPage"></activity>
    </application>

</manifest>